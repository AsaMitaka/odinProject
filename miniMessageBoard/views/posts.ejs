<%- include('../views/header.ejs') %>
<div class="posts">
  <h1><%=pageTitle %></h1>
  <% data.forEach(function(post) { %>
  <div class="post">
    <h2><%= post.title %></h2>
    <p><%= post.descr %></p>
    <a class="post__link" href="/posts/<%= post.id %>">Go to post</a>
  </div>
  <% }); %>
</div>
<%- include('../views/footer.ejs') %>
